{
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "jestrunner.jestCommand": "dotenv -e .env.test.local -- node --experimental-vm-modules 'node_modules/.bin/jest'",
  //// "jestrunner.jestCommand": "dotenv -e .env.test.local -- nox  --experimental-vm-modules 'node_modules/.bin/jest'",
  "jestrunner.configPath": "jest.config.js",
  "typescript.tsdk": "node_modules/typescript/lib",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[javascript][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  }
}
