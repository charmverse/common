{
  "name": "utilities-string",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utilities-string/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputFile}"],
      "options": {
        "main": "packages/utilities-string/src/index.ts",
        "tsConfig": "./tsconfig.json",
        "outputPath": "dist/utilities-string",
        "rootDir": "packages/utilities-string/src",
        "clean": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utilities-string/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/utilities-string/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
